package drupalservices.apis;

public class UserLogoutApi extends ServicesApis{
	
	
	public UserLogoutApi(String sessionid)	
	{
		apiName = "user.logout";
		xml = "<?xml version=\"1.0\"?>" +
				"<methodCall><methodName>user.logout</methodName>" +
					"<params>" +
						"<param>" +
							"<value><string>"+sessionid+"</string></value>" +
						"</param>" +
					"</params>" +
				"</methodCall>";		
	}
	

}
